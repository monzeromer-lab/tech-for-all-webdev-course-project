<div class="container post">
<% if (post) { %>
 <h1 class="head posttitlet"><%= post.title %> </h1>
<div class="postpart">
    <img class="postimage" src="<%= post.image %>" height="146px" width="218px">
  <p class="postbodypage"><%= post.body %> </p>
  <span class="postdatepage"><%= post.createdAt %> </span>
  </div>
  <a href="/edit/<%= post.id %>" class="submit">Edit</a>
  <a href="/post/d/<%= post.id %>" class="submit">Delete</a>
</div>
<% } else { %>
 <h1> Wrong Id </h1>
<% } %>


